{
  "name": "pbs",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.8.8",
    "turbo": "^1.13.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@10.12.0"
}
